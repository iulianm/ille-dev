    "express-session": "^1.15.6",
    "lodash": "^4.17.10",
    "mocha": "^5.2.0",
    "mongoose": "^5.2.2",
    "morgan": "^1.9.0",
    "node-localstorage": "^1.3.1",
    "nodemon": "^1.17.5",
    "passport": "^0.4.0",
